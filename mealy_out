v0x58467b530f70_0 .var "next_state", 1 0;
E_0x58467b4dd950 .event anyedge, v0x58467b530e60_0, v0x58467b530d80_0;
E_0x58467b516530 .event posedge, v0x58467b504670_0, v0x58467b504540_0;
    .scope S_0x58467b51a410;
T_0 ;
    %wait E_0x58467b516530;
    %load/vec4 v0x58467b504670_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x58467b530d80_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x58467b530f70_0;
    %assign/vec4 v0x58467b530d80_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x58467b51a410;
T_1 ;
    %wait E_0x58467b4dd950;
    %load/vec4 v0x58467b530d80_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %load/vec4 v0x58467b530d80_0;
    %store/vec4 v0x58467b530f70_0, 0, 2;
    %jmp T_1.4;
T_1.0 ;
    %load/vec4 v0x58467b530e60_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.5, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x58467b530f70_0, 0, 2;
    %jmp T_1.6;
T_1.5 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x58467b530f70_0, 0, 2;
T_1.6 ;
    %jmp T_1.4;
T_1.1 ;
    %load/vec4 v0x58467b530e60_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.7, 4;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x58467b530f70_0, 0, 2;
    %jmp T_1.8;
T_1.7 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x58467b530f70_0, 0, 2;
T_1.8 ;
    %jmp T_1.4;
T_1.2 ;
