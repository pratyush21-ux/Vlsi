    %flag_set/vec4 8;
    %jmp/0xz  T_1.9, 8;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x58467b530f70_0, 0, 2;
    %jmp T_1.10;
T_1.9 ;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x58467b530f70_0, 0, 2;
T_1.10 ;
    %jmp T_1.4;
T_1.4 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x58467b51a410;
T_2 ;
    %wait E_0x58467b4dd950;
    %load/vec4 v0x58467b530d80_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
