#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5e674f387e50 .scope module, "Clock_count2_test" "Clock_count2_test" 2 2;
 .timescale -9 -12;
v0x5e674f39e700_0 .var "clk", 0 0;
v0x5e674f39e7c0_0 .net "cn", 5 0, v0x5e674f36fa20_0;  1 drivers
v0x5e674f39e860_0 .net "hr", 5 0, v0x5e674f39e390_0;  1 drivers
v0x5e674f39e930_0 .net "min", 5 0, v0x5e674f39e4c0_0;  1 drivers
v0x5e674f39ea00_0 .var "rst", 0 0;
S_0x5e674f387fe0 .scope module, "uut" "Clock_count1" 2 8, 3 1 0, S_0x5e674f387e50;
 .timescale 0 0;
    .port_info 0 /OUTPUT 6 "cn";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "rst";
    .port_info 3 /OUTPUT 6 "min";
    .port_info 4 /OUTPUT 6 "hr";
v0x5e674f36f920_0 .net "clk", 0 0, v0x5e674f39e700_0;  1 drivers
v0x5e674f36fa20_0 .var "cn", 5 0;
v0x5e674f370870_0 .var "count_hr", 5 0;
v0x5e674f370940_0 .var "count_min", 5 0;
v0x5e674f39e390_0 .var "hr", 5 0;
v0x5e674f39e4c0_0 .var "min", 5 0;
v0x5e674f39e5a0_0 .net "rst", 0 0, v0x5e674f39ea00_0;  1 drivers
E_0x5e674f3819e0/0 .event negedge, v0x5e674f39e5a0_0;
E_0x5e674f3819e0/1 .event posedge, v0x5e674f36f920_0;
E_0x5e674f3819e0 .event/or E_0x5e674f3819e0/0, E_0x5e674f3819e0/1;
S_0x5e674f348490 .scope begin, "FOR_HR_CLOCK" "FOR_HR_CLOCK" 3 51, 3 51 0, S_0x5e674f387fe0;
 .timescale 0 0;
S_0x5e674f348670 .scope begin, "FOR_MIN_CLOCK" "FOR_MIN_CLOCK" 3 30, 3 30 0, S_0x5e674f387fe0;
 .timescale 0 0;
S_0x5e674f373af0 .scope begin, "FOR_SEC_CLOCK" "FOR_SEC_CLOCK" 3 9, 3 9 0, S_0x5e674f387fe0;
 .timescale 0 0;
    .scope S_0x5e674f387fe0;
T_0 ;
    %wait E_0x5e674f3819e0;
    %fork t_1, S_0x5e674f373af0;
    %jmp t_0;
    .scope S_0x5e674f373af0;
t_1 ;
    %load/vec4 v0x5e674f39e5a0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x5e674f36fa20_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x5e674f370940_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5e674f36fa20_0;
    %cmpi/e 5, 0, 6;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x5e674f36fa20_0, 0;
    %pushi/vec4 1, 0, 6;
    %assign/vec4 v0x5e674f370940_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5e674f36fa20_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x5e674f36fa20_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x5e674f370940_0, 0;
T_0.3 ;
T_0.1 ;
    %end;
    .scope S_0x5e674f387fe0;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5e674f387fe0;
T_1 ;
    %wait E_0x5e674f3819e0;
    %fork t_3, S_0x5e674f348670;
    %jmp t_2;
    .scope S_0x5e674f348670;
t_3 ;
    %load/vec4 v0x5e674f39e5a0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x5e674f39e4c0_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x5e674f370870_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5e674f370940_0;
    %cmpi/e 1, 0, 6;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x5e674f39e4c0_0;
    %cmpi/e 5, 0, 6;
    %jmp/0xz  T_1.4, 4;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x5e674f39e4c0_0, 0;
    %pushi/vec4 1, 0, 6;
    %assign/vec4 v0x5e674f370870_0, 0;
    %jmp T_1.5;
T_1.4 ;
    %load/vec4 v0x5e674f39e4c0_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x5e674f39e4c0_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x5e674f370870_0, 0;
T_1.5 ;
T_1.2 ;
T_1.1 ;
    %end;
    .scope S_0x5e674f387fe0;
t_2 %join;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5e674f387fe0;
T_2 ;
    %wait E_0x5e674f3819e0;
    %fork t_5, S_0x5e674f348490;
    %jmp t_4;
    .scope S_0x5e674f348490;
t_5 ;
    %load/vec4 v0x5e674f39e5a0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x5e674f39e390_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x5e674f370870_0;
    %cmpi/e 1, 0, 6;
    %jmp/0xz  T_2.2, 4;
    %load/vec4 v0x5e674f39e390_0;
    %cmpi/e 23, 0, 6;
    %jmp/0xz  T_2.4, 4;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x5e674f39e390_0, 0;
    %jmp T_2.5;
T_2.4 ;
    %load/vec4 v0x5e674f39e390_0;
