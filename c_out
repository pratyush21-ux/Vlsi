t_4 %join;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5e674f387e50;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x5e674f39e700_0;
    %inv;
    %store/vec4 v0x5e674f39e700_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5e674f387e50;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e674f39e700_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e674f39ea00_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e674f39ea00_0, 0, 1;
    %delay 37000000, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x5e674f387e50;
T_5 ;
    %vpi_call 2 28 "$monitor", "Time: %0t | Seconds: %0d | Minutes: %0d | Hours: %0d", $time, v0x5e674f39e7c0_0, v0x5e674f39e930_0, v0x5e674f39e860_0 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
