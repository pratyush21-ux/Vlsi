:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5ea952b59d90 .scope module, "counter_4bit_updown_test" "counter_4bit_updown_test" 2 4;
 .timescale -9 -12;
v0x5ea952b6fc50_0 .var "cin", 3 0;
v0x5ea952b6fd30_0 .var "clk", 0 0;
v0x5ea952b6fe00_0 .net "cn", 3 0, v0x5ea952b6f760_0;  1 drivers
v0x5ea952b6ff00_0 .var "en", 0 0;
v0x5ea952b6ffd0_0 .var "load", 0 0;
v0x5ea952b700c0_0 .var "rst", 0 0;
v0x5ea952b70190_0 .var "ud", 0 0;
S_0x5ea952b59f20 .scope module, "uut" "counter_4bit_updown" 2 8, 3 16 0, S_0x5ea952b59d90;
 .timescale 0 0;
    .port_info 0 /OUTPUT 4 "cn";
    .port_info 1 /INPUT 4 "cin";
    .port_info 2 /INPUT 1 "clk";
    .port_info 3 /INPUT 1 "rst";
    .port_info 4 /INPUT 1 "ud";
    .port_info 5 /INPUT 1 "en";
    .port_info 6 /INPUT 1 "load";
v0x5ea952b1efb0_0 .net "cin", 3 0, v0x5ea952b6fc50_0;  1 drivers
v0x5ea952b6f6a0_0 .net "clk", 0 0, v0x5ea952b6fd30_0;  1 drivers
v0x5ea952b6f760_0 .var "cn", 3 0;
v0x5ea952b6f820_0 .net "en", 0 0, v0x5ea952b6ff00_0;  1 drivers
v0x5ea952b6f8e0_0 .net "load", 0 0, v0x5ea952b6ffd0_0;  1 drivers
v0x5ea952b6f9f0_0 .net "rst", 0 0, v0x5ea952b700c0_0;  1 drivers
v0x5ea952b6fab0_0 .net "ud", 0 0, v0x5ea952b70190_0;  1 drivers
E_0x5ea952b1f070/0 .event negedge, v0x5ea952b6f9f0_0;
E_0x5ea952b1f070/1 .event posedge, v0x5ea952b6f6a0_0;
E_0x5ea952b1f070 .event/or E_0x5ea952b1f070/0, E_0x5ea952b1f070/1;
    .scope S_0x5ea952b59f20;
T_0 ;
    %wait E_0x5ea952b1f070;
    %load/vec4 v0x5ea952b6f9f0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5ea952b6f760_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5ea952b6f820_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5ea952b6f8e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %load/vec4 v0x5ea952b1efb0_0;
    %assign/vec4 v0x5ea952b6f760_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x5ea952b6fab0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
    %load/vec4 v0x5ea952b6f760_0;
    %cmpi/e 15, 0, 4;
    %jmp/0xz  T_0.8, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5ea952b6f760_0, 0;
    %jmp T_0.9;
T_0.8 ;
    %load/vec4 v0x5ea952b6f760_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x5ea952b6f760_0, 0;
T_0.9 ;
    %jmp T_0.7;
T_0.6 ;
    %load/vec4 v0x5ea952b6f760_0;
    %cmpi/e 0, 0, 4;
    %jmp/0xz  T_0.10, 4;
    %pushi/vec4 15, 0, 4;
    %assign/vec4 v0x5ea952b6f760_0, 0;
    %jmp T_0.11;
T_0.10 ;
    %load/vec4 v0x5ea952b6f760_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x5ea952b6f760_0, 0;
T_0.11 ;
T_0.7 ;
T_0.5 ;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5ea952b59d90;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ea952b6fd30_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5ea952b6fd30_0;
    %inv;
    %store/vec4 v0x5ea952b6fd30_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5ea952b59d90;
T_2 ;
    %vpi_call 2 23 "$monitor", "rst=%b,en=%b,ud=%b,load=%b,cin=%b,cn=%b", v0x5ea952b700c0_0, v0x5ea952b6ff00_0, v0x5ea952b70190_0, v0x5ea952b6ffd0_0, v0x5ea952b6fc50_0, v0x5ea952b6fe00_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ea952b700c0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ea952b700c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ea952b6ff00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ea952b70190_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ea952b6ffd0_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5ea952b6fc50_0, 0, 4;
    %delay 60000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ea952b700c0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ea952b700c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ea952b6ff00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ea952b70190_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ea952b6ffd0_0, 0, 1;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x5ea952b6fc50_0, 0, 4;
    %delay 60000, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_2;
