#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5612fdc00c50 .scope module, "Ha_bh_test" "Ha_bh_test" 2 2;
 .timescale -9 -12;
v0x5612fdc13bc0_0 .var "a", 0 0;
v0x5612fdc13c80_0 .var "b", 0 0;
v0x5612fdc13d50_0 .net "c", 0 0, v0x5612fdc13980_0;  1 drivers
v0x5612fdc13e50_0 .net "s", 0 0, v0x5612fdc13a50_0;  1 drivers
S_0x5612fdc00de0 .scope module, "uut" "Ha_bh" 2 5, 3 1 0, S_0x5612fdc00c50;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "s";
    .port_info 1 /OUTPUT 1 "c";
    .port_info 2 /INPUT 1 "a";
    .port_info 3 /INPUT 1 "b";
v0x5612fdbc7490_0 .net "a", 0 0, v0x5612fdc13bc0_0;  1 drivers
v0x5612fdc138c0_0 .net "b", 0 0, v0x5612fdc13c80_0;  1 drivers
v0x5612fdc13980_0 .var "c", 0 0;
v0x5612fdc13a50_0 .var "s", 0 0;
E_0x5612fdbc7f20 .event anyedge, v0x5612fdc138c0_0, v0x5612fdbc7490_0;
    .scope S_0x5612fdc00de0;
T_0 ;
    %wait E_0x5612fdbc7f20;
    %load/vec4 v0x5612fdbc7490_0;
    %load/vec4 v0x5612fdc138c0_0;
    %xor;
    %store/vec4 v0x5612fdc13a50_0, 0, 1;
    %load/vec4 v0x5612fdbc7490_0;
    %load/vec4 v0x5612fdc138c0_0;
    %and;
    %store/vec4 v0x5612fdc13980_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5612fdc00c50;
T_1 ;
    %vpi_call 2 7 "$monitor", "s=%b,c=%b,a=%b,b=%b", v0x5612fdc13e50_0, v0x5612fdc13d50_0, v0x5612fdc13bc0_0, v0x5612fdc13c80_0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5612fdc00c50;
T_2 ;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5612fdc13bc0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5612fdc13c80_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5612fdc13bc0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5612fdc13c80_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5612fdc13bc0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5612fdc13c80_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5612fdc13bc0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5612fdc13c80_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x5612fdc00c50;
T_3 ;
    %vpi_call 2 17 "$dumpfile", "Hawave.vcd" {0 0 0};
    %vpi_call 2 18 "$dumpvars" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "Ha_bh_test.v";
    "Ha_bh.v";
