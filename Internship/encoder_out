#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x59c2d3fbc7d0 .scope module, "encoder_4x2_test" "encoder_4x2_test" 2 3;
 .timescale -9 -12;
v0x59c2d3fd0fd0_0 .var "E", 0 0;
v0x59c2d3fd1090_0 .var "I", 3 0;
v0x59c2d3fd1160_0 .net "V", 0 0, v0x59c2d3fd0d70_0;  1 drivers
v0x59c2d3fd1260_0 .net "Y", 1 0, v0x59c2d3fd0e40_0;  1 drivers
S_0x59c2d3fbc960 .scope module, "uut" "encoder_4x2" 2 10, 3 1 0, S_0x59c2d3fbc7d0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "I";
    .port_info 1 /OUTPUT 2 "Y";
    .port_info 2 /INPUT 1 "E";
    .port_info 3 /OUTPUT 1 "V";
v0x59c2d3f81490_0 .net "E", 0 0, v0x59c2d3fd0fd0_0;  1 drivers
v0x59c2d3fd0c90_0 .net "I", 3 0, v0x59c2d3fd1090_0;  1 drivers
v0x59c2d3fd0d70_0 .var "V", 0 0;
v0x59c2d3fd0e40_0 .var "Y", 1 0;
E_0x59c2d3f81f50 .event anyedge, v0x59c2d3f81490_0, v0x59c2d3fd0c90_0;
    .scope S_0x59c2d3fbc960;
T_0 ;
    %wait E_0x59c2d3f81f50;
    %load/vec4 v0x59c2d3f81490_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x59c2d3fd0c90_0;
    %parti/s 1, 3, 3;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %pushi/vec4 6, 0, 3;
    %split/vec4 1;
    %store/vec4 v0x59c2d3fd0d70_0, 0, 1;
    %store/vec4 v0x59c2d3fd0e40_0, 0, 2;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x59c2d3fd0c90_0;
    %parti/s 1, 2, 3;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %pushi/vec4 5, 0, 3;
    %split/vec4 1;
    %store/vec4 v0x59c2d3fd0d70_0, 0, 1;
    %store/vec4 v0x59c2d3fd0e40_0, 0, 2;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x59c2d3fd0c90_0;
    %parti/s 1, 1, 2;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
    %pushi/vec4 3, 0, 3;
    %split/vec4 1;
    %store/vec4 v0x59c2d3fd0d70_0, 0, 1;
    %store/vec4 v0x59c2d3fd0e40_0, 0, 2;
    %jmp T_0.7;
T_0.6 ;
    %load/vec4 v0x59c2d3fd0c90_0;
    %parti/s 1, 0, 2;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.8, 8;
    %pushi/vec4 1, 0, 3;
    %split/vec4 1;
    %store/vec4 v0x59c2d3fd0d70_0, 0, 1;
    %store/vec4 v0x59c2d3fd0e40_0, 0, 2;
    %jmp T_0.9;
T_0.8 ;
    %pushi/vec4 0, 0, 3;
    %split/vec4 1;
    %store/vec4 v0x59c2d3fd0d70_0, 0, 1;
    %store/vec4 v0x59c2d3fd0e40_0, 0, 2;
T_0.9 ;
T_0.7 ;
T_0.5 ;
T_0.3 ;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 3;
    %split/vec4 1;
    %store/vec4 v0x59c2d3fd0d70_0, 0, 1;
    %store/vec4 v0x59c2d3fd0e40_0, 0, 2;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x59c2d3fbc7d0;
T_1 ;
    %vpi_call 2 18 "$monitor", "Time = %0t | En = %b | I = %b | Y = %b | V = %b", $time, v0x59c2d3fd0fd0_0, v0x59c2d3fd1090_0, v0x59c2d3fd1260_0, v0x59c2d3fd1160_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x59c2d3fd0fd0_0, 0, 1;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x59c2d3fd1090_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x59c2d3fd1090_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x59c2d3fd0fd0_0, 0, 1;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x59c2d3fd1090_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x59c2d3fd1090_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x59c2d3fd1090_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x59c2d3fd1090_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x59c2d3fd1090_0, 0, 4;
    %delay 10000, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
