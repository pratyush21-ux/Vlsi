#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5e8bbfda23e0 .scope module, "decoder_2x4_test" "decoder_2x4_test" 2 3;
 .timescale -9 -12;
v0x5e8bbfdb5c40_0 .var "E", 0 0;
v0x5e8bbfdb5d10_0 .var "I", 1 0;
v0x5e8bbfdb5de0_0 .net "Y", 3 0, v0x5e8bbfdb5b00_0;  1 drivers
S_0x5e8bbfda2570 .scope module, "uut" "decoder_2x4" 2 8, 3 1 0, S_0x5e8bbfda23e0;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "I";
    .port_info 1 /INPUT 1 "E";
    .port_info 2 /OUTPUT 4 "Y";
v0x5e8bbfda27c0_0 .net "E", 0 0, v0x5e8bbfdb5c40_0;  1 drivers
v0x5e8bbfdb5a20_0 .net "I", 1 0, v0x5e8bbfdb5d10_0;  1 drivers
v0x5e8bbfdb5b00_0 .var "Y", 3 0;
E_0x5e8bbfd69070 .event anyedge, v0x5e8bbfda27c0_0, v0x5e8bbfdb5a20_0;
    .scope S_0x5e8bbfda2570;
T_0 ;
    %wait E_0x5e8bbfd69070;
    %load/vec4 v0x5e8bbfda27c0_0;
    %load/vec4 v0x5e8bbfdb5a20_0;
    %concat/vec4; draw_concat_vec4
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 3;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 6, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 7, 0, 3;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5e8bbfdb5b00_0, 0, 4;
    %jmp T_0.5;
T_0.0 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x5e8bbfdb5b00_0, 0, 4;
    %jmp T_0.5;
T_0.1 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x5e8bbfdb5b00_0, 0, 4;
    %jmp T_0.5;
T_0.2 ;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x5e8bbfdb5b00_0, 0, 4;
    %jmp T_0.5;
T_0.3 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x5e8bbfdb5b00_0, 0, 4;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5e8bbfda23e0;
T_1 ;
    %vpi_call 2 14 "$monitor", "Time = %0t | En = %b | I = %b | Y = %b", $time, v0x5e8bbfdb5c40_0, v0x5e8bbfdb5d10_0, v0x5e8bbfdb5de0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e8bbfdb5c40_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5e8bbfdb5d10_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5e8bbfdb5d10_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x5e8bbfdb5d10_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x5e8bbfdb5d10_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e8bbfdb5c40_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5e8bbfdb5d10_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5e8bbfdb5d10_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x5e8bbfdb5d10_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x5e8bbfdb5d10_0, 0, 2;
    %delay 10000, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "decoder_2x4_test.v";
    "decoder_2x4.v";
