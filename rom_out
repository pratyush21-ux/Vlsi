#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x57178b3bd560 .scope module, "rom_4x4_async_test" "rom_4x4_async_test" 2 2;
 .timescale -9 -12;
v0x57178b3cfd10_0 .var "address", 1 0;
v0x57178b3cfe00_0 .net "data_out", 3 0, v0x57178b3cfbf0_0;  1 drivers
S_0x57178b3bd6f0 .scope module, "uut" "rom_4x4_async" 2 5, 3 1 0, S_0x57178b3bd560;
 .timescale 0 0;
    .port_info 0 /OUTPUT 4 "data_out";
    .port_info 1 /INPUT 2 "address";
v0x57178b3bd8f0 .array "ROM", 3 0, 3 0;
v0x57178b383490_0 .net "address", 1 0, v0x57178b3cfd10_0;  1 drivers
v0x57178b3cfbf0_0 .var "data_out", 3 0;
E_0x57178b3bc470 .event anyedge, v0x57178b383490_0;
    .scope S_0x57178b3bd6f0;
T_0 ;
    %pushi/vec4 14, 0, 4;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x57178b3bd8f0, 4, 0;
    %pushi/vec4 2, 0, 4;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x57178b3bd8f0, 4, 0;
    %pushi/vec4 15, 0, 4;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x57178b3bd8f0, 4, 0;
    %pushi/vec4 4, 0, 4;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x57178b3bd8f0, 4, 0;
    %end;
    .thread T_0;
    .scope S_0x57178b3bd6f0;
T_1 ;
    %wait E_0x57178b3bc470;
    %load/vec4 v0x57178b383490_0;
    %pad/u 4;
    %ix/vec4 4;
    %load/vec4a v0x57178b3bd8f0, 4;
    %store/vec4 v0x57178b3cfbf0_0, 0, 4;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x57178b3bd560;
T_2 ;
    %vpi_call 2 10 "$monitor", "Time = %0t | Address = %b | Data Out = %b", $time, v0x57178b3cfd10_0, v0x57178b3cfe00_0 {0 0 0};
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x57178b3cfd10_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x57178b3cfd10_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x57178b3cfd10_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x57178b3cfd10_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x57178b3cfd10_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x57178b3cfd10_0, 0, 2;
    %delay 10000, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "rom_4x4_async_test.v";
    "rom_4x4_async.v";
